.dashboard-rants
  %section.clearfix
    .latest_rants
      %h1 Latest Rants
      %hr
      - if @all_rants != nil
        = render @all_rants
    .my_rants
      %h1#my_rants My Rants
      - if current_user.rants != nil
        = render current_user.rants.order(created_at: :desc)
    .mentioned
      %h1 Mentioned
      - if @mentioned_rants != nil
        = render @mentioned_rants
